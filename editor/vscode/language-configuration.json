{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["\"", "\""],
    ["'", "'"],
    ["(", ")"],
    ["[", "]"],
    ["{", "}"]
  ],
  "indentationRules": {
    "increaseIndentPattern": ".*:(\\s*#\\s\\S.*)?$",
    "decreaseIndentPattern": "^\\s*(end|else|elif|)$"
  }
}
